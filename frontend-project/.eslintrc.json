{
    "extends": ["react-app", "airbnb", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error", { "semi": true, "singleQuote": true }],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "import/prefer-default-export": "off",
      "no-console": "warn"
    }
  }